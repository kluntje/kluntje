{
  "name": "@kluntje/js-utils",
  "version": "0.4.0",
  "description": "Collection of js helper functions",
  "author": "Frederik Riewerts <frederik.riewerts@gmail.com>",
  "contributors": [
    "Alexander HÃ¤usler",
    "Ingo Fahrentholz",
    "Frederik Riewerts",
    "Bert-Ulrich Baumann"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./lib/api-helpers": {
      "require": "./lib/api-helpers/index.js",
      "import": "./lib/api-helpers/index.mjs"
    },
    "./lib/array-helpers": {
      "require": "./lib/array-helpers/index.js",
      "import": "./lib/array-helpers/index.mjs"
    },
    "./lib/date-helpers": {
      "require": "./lib/date-helpers/index.js",
      "import": "./lib/date-helpers/index.mjs",
      "types": "./lib/date-helpers/index.d.ts"
    },
    "./lib/dom-helpers": {
      "require": "./lib/dom-helpers/index.js",
      "import": "./lib/dom-helpers/index.mjs"
    },
    "./lib/function-helpers": {
      "require": "./lib/function-helpers/index.js",
      "import": "./lib/function-helpers/index.mjs"
    },
    "./lib/function-helpers/decorators": {
      "require": "./lib/function-helpers/decorators/index.js",
      "import": "./lib/function-helpers/decorators/index.mjs"
    },
    "./lib/object-helpers": {
      "require": "./lib/object-helpers/index.js",
      "import": "./lib/object-helpers/index.mjs"
    },
    "./lib/string-helpers": {
      "require": "./lib/string-helpers/index.js",
      "import": "./lib/string-helpers/index.mjs"
    }
  },
  "repository": "https://github.com/kluntje/kluntje/tree/develop/packages/js-utils",
  "scripts": {
    "build": "npm-run-all clean-dist generate-declarations build:*",
    "clean-dist": "rimraf lib",
    "generate-declarations": "tsc -d --outDir lib --emitDeclarationOnly",
    "build:index": "rollup -c build.js --environment TARGET:index",
    "build:apiHelpers": "rollup -c build.js --environment TARGET:apiHelpers",
    "build:arrayHelpers": "rollup -c build.js --environment TARGET:arrayHelpers",
    "build:dateHelpers": "rollup -c build.js --environment TARGET:dateHelpers",
    "build:domHelpers": "rollup -c build.js --environment TARGET:domHelpers",
    "build:functionHelpers": "rollup -c build.js --environment TARGET:functionHelpers",
    "build:functionHelpers/decorators": "rollup -c build.js --environment TARGET:functionHelpers/decorators",
    "build:objectHelpers": "rollup -c build.js --environment TARGET:objectHelpers",
    "build:stringHelpers": "rollup -c build.js --environment TARGET:stringHelpers",
    "test": "jest"
  }
}
